<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 引入配置文件 -->
    <properties url="file:////Users/finley/Finley/workspace/java/suishoushu/src/main/profile/dev/config.properties"/>

    <classPathEntry location="${classPath}" />

    <context id="suishoushu" targetRuntime="MyBatis3">

        <!-- 注释 -->
        <commentGenerator >
            <property name="suppressAllComments" value="false"/><!-- 是否取消注释 -->
            <property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳-->
        </commentGenerator>

        <!-- jdbc连接 -->
        <jdbcConnection driverClass="${jdbc.driver}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.user}"
                        password="${jdbc.password}">
        </jdbcConnection>

        <!-- 类型转换 -->
        <!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->
        <!--
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        -->

        <javaModelGenerator targetPackage="com.changyi.fi.model" targetProject="${project}">
            <!-- 是否在当前路径下新加一层schema,eg：fase路径com.oop.eksp.user.model， true:com.oop.eksp.user.model.[schemaName] -->
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.changyi.fi.dao"  targetProject="${project}">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.changyi.fi.dao"  targetProject="${project}">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table schema="${dev_schema}" tableName="INVOICE" domainObjectName="InvoicePO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="true">
        </table>

        <table schema="${dev_schema}" tableName="ENTERPRISE" domainObjectName="EnterprisePO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="true">
        </table>

        <table schema="${dev_schema}" tableName="ENTERPRISE_MODIFY_HISTORY" domainObjectName="EnterpriseHistoryPO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="true">
        </table>

        <table schema="${dev_schema}" tableName="MERCHANT" domainObjectName="MerchantPO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="true">
        </table>

        <table schema="${dev_schema}" tableName="MERCHANT_INVOICE" domainObjectName="MerchantInvoicePO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="true">
        </table>

        <!--
        <table schema="${dev_schema}" tableName="MERCHANT_TYPE" domainObjectName="MerchantTypePO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        -->

        <table schema="${dev_schema}" tableName="CUSTOMER" domainObjectName="CustomerPO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="true">
        </table>

        <table schema="${dev_schema}" tableName="CUSTOMER_LOGIN" domainObjectName="CustomerLoginPO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false">
        </table>

        <table schema="${dev_schema}" tableName="V_INVOICE" domainObjectName="VInvoicePO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="true">
        </table>

        <table schema="${dev_schema}" tableName="SYS_ACCESS" domainObjectName="SysAccessPO" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="true"
               enableUpdateByExample="false">
        </table>

    </context>
</generatorConfiguration>